<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.whack_a_mole">

    <application
        android:allowBackup="true"
        android:icon="@drawable/launchicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/launchericon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WhackAMole">
        <activity android:name=".Options"></activity>
        <activity android:name=".WMTitleScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HighScores" />
        <activity android:name=".GameOver" />
        <activity android:name=".Game" />
    </application>

</manifest>